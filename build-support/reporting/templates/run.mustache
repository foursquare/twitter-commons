<div class="run">
{{#no_such_run}}
<div class="no-such-run">No run found with id {{.}}</div>
{{/no_such_run}}
{{^no_such_run}}
<div class="run-info">
{{#run_info}}
<div class="timestamp-text">Pants run at {{timestamp_text}}</div>
<div class="cmd-line-label">Command line:</div>
<div class="cmd-line monospace">{{cmd_line}}</div>
</div>
<div id="report_{{id}}"></div>
<script>
  $(function() {
    pants.tailer.startTailing('run_{{id}}_root', '{{report_path}}', '#report_{{id}}');
  });
</script>
{{/run_info}}
{{/no_such_run}}
</div>
